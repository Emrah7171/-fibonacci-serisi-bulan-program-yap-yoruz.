import java.util.Scanner;

public class neue1 {
    static int fib(int n){
        if (n==1||n==2){
            return 1;

        }else {
            return fib(n-1)+fib(n-2);
        }
    }
    public static void main(String[] args) {
        Scanner neu=new Scanner(System.in);
        System.out.print("SAYI GIRINIZ: ");
        int n1=neu.nextInt();
        System.out.println(fib(n1));

    }
}
